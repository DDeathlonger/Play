    "adaptive_grids": {
        "description": "Dynamic coordinate generation based on detected screen size",
        "algorithms": {
            "left_panel_generator": {
                "description": "Generate left panel coordinates based on screen size",
                "formula": {
                    "start_x": "screen_width * 0.05",
                    "start_y": "screen_height * 0.1",
                    "step_x": "40",
                    "step_y": "40",
                    "columns": "3",
                    "rows": "screen_height < 800 ? 6 : 8"
                }
            },
            "viewport_generator": {
                "description": "Generate viewport coordinates for 3D area",
                "formula": {
                    "center_x": "screen_width * 0.4",
                    "center_y": "screen_height * 0.4",
                    "radius": "min(screen_width, screen_height) * 0.1"
                }
            },
            "bottom_toolbar_generator": {
                "description": "Generate bottom toolbar coordinates",
                "formula": {
                    "start_x": "screen_width * 0.05",
                    "y_position": "screen_height * 0.8",
                    "button_width": "60",
                    "button_count": "screen_width > 1920 ? 8 : 6"
                }
            }
        }
    },
